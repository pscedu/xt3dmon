# $Id$

opt frames,nodeanim,fancy,cores,govern,tween
panel legend

dmode job
vmode wione
bird
setcap "Start..."
stall 2
setcap "Viewing jobs in wired mode"
orbit +x 3.0 55.0

setcap "Changing to physical mode"
vmode phys
bird
stall 2
setcap "Changing to temperature view"
dmode temp
stall 2
setcap "Viewing node temperatures in physical mode"
orbit +x 2.0 32.0

setcap "Changing to wired mode"
bird wione
vmode wione
bird
stall 2
setcap "Changing to job view"
dmode job
stall 2

exit
